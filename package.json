{
  "name": "@egillanton/payday-api-client",
  "version": "0.0.1",
  "description": "A JavaScript / TypeScript API Client for Payday.",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "build:esm": "tsc --module es2015 --target es5 --outDir esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egillanton/payday-api-client.git"
  },
  "author": "Egill Anton Hlöðversson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/egillanton/payday-api-client/issues"
  },
  "homepage": "https://github.com/egillanton/payday-api-client#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "node-fetch": "^3.3.1"
  }
}
